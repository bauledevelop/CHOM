<!DOCTYPE html>
<html lang="en"> 

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="author" content="pixelstrap">
    <link rel="icon" href="~/Client/dict/images/logo.svg" />
    <link rel="shortcut icon" href="~/Client/dict/images/logo.svg" type="image/x-icon" />
    <meta itemprop="name" content="CHOM Design" />
    <meta itemprop="image" content="https://chomdegisn.bsite.net/Client/dict/images/logo.svg" />

    <meta property="og:type" content="website" />
    <meta property="og:title" content="CHOM Design" />
    <meta property="og:url" content="https://chomdegisn.bsite.net/" />
    <meta property="og:site_name" content="CHOM Design"/>
    <meta property="og:description" content="CHOM Design" />
    <meta property="og:image" content="https://chomdegisn.bsite.net/Client/dict/images/logo.svg" />
    <meta property="og:image:url" content="https://chomdegisn.bsite.net/Client/dict/images/logo.svg" />

    <meta name="twitter:card" content="https://chomdegisn.bsite.net/" />
    <meta name="twitter:title" content="CHOM Design" />
    <meta name="twitter:image" content="https://chomdegisn.bsite.net/Client/dict/images/logo.svg" />

    <title>@ViewBag.Title</title>
    <!-- Google font-->
    <link rel="preconnect" href="https://fonts.googleapis.com/">
    <link rel="preconnect" href="https://fonts.gstatic.com/" crossorigin="">
    <link href="https://fonts.googleapis.com/css2?family=Rubik:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,300;1,400;1,500;1,600;1,700;1,800;1,900&amp;display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&amp;display=swap" rel="stylesheet">
    <link rel="stylesheet" src="~/vendor/fontawesome-free-6.2.0-web/css/all.css"/>
    <!-- ico-font-->
    <link rel="stylesheet" type="text/css" href="~/Admin/dict/css/vendors/icofont.css">
    <!-- Themify icon-->
    <link rel="stylesheet" type="text/css" href="~/Admin/dict/css/vendors/themify.css">
    <!-- Flag icon-->
    <link rel="stylesheet" type="text/css" href="~/Admin/dict/css/vendors/flag-icon.css">
    <link rel="stylesheet" type="text/css" href="~/Admin/dict/css/vendors/dropzone.css">
    <!-- Feather icon-->
    <link rel="stylesheet" type="text/css" href="~/Admin/dict/css/vendors/feather-icon.css">
    <!-- Plugins css start-->
    <link rel="stylesheet" type="text/css" href="~/Admin/dict/css/vendors/scrollbar.css">
    <link rel="stylesheet" type="text/css" href="~/Admin/dict/css/vendors/datatables.css">
    <!-- Plugins css Ends-->
    <!-- Bootstrap css-->
    <link rel="stylesheet" type="text/css" href="~/Admin/dict/css/vendors/bootstrap.css">
    <!-- App css-->
    <link rel="stylesheet" type="text/css" href="~/Admin/dict/css/style.css">
    <!-- Responsive css-->
    <link rel="stylesheet" type="text/css" href="~/Admin/dict/css/responsive.css">
    <link rel="stylesheet" type="text/css" href="~/Admin/css/all.css" />
    @await RenderSectionAsync("css",required: false)
  </head>
  <body>     
    <!-- tap on top starts-->
    <div class="tap-top"><i data-feather="chevrons-up"></i></div>
    <!-- tap on tap ends-->
    @RenderBody()
    <!-- latest jquery-->
    <script src="~/Admin/dict/js/jquery-3.5.1.min.js"></script>
    <!-- Bootstrap js-->
    <script src="~/Admin/dict/js/bootstrap/bootstrap.bundle.min.js"></script>
    <!-- feather icon js-->
    <script src="~/Admin/dict/js/icons/feather-icon/feather.min.js"></script>
    <script src="~/Admin/dict/js/icons/feather-icon/feather-icon.js"></script>
    <!-- scrollbar js-->
    <!-- Sidebar jquery-->
    <script src="~/vendor/fontawesome-free-6.2.0-web/js/all.min.js"></script>
    <script src="~/Admin/dict/js/config.js"></script>
    <!-- Plugins JS start-->
    <script src="~/Admin/dict/js/datatable/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Admin/dict/js/datatable/datatables/datatable.custom.js"></script>
    <!-- Plugins JS Ends-->
    <!-- Theme js-->
    <script src="~/Admin/dict/js/dropzone/dropzone.js"></script>
    <script src="~/Admin/dict/js/dropzone/dropzone-script.js"></script>
    <!-- login js-->
    <!-- Plugin used-->
    <script src="~/Admin/dict/js/script.js"></script>
    
    <script src="~/Admin/dict/js/datepicker/date-picker/datepicker.js"></script>
    <script src="~/Admin/dict/js/datepicker/date-picker/datepicker.en.js"></script>
    <script src="~/Admin/dict/js/datepicker/date-picker/datepicker.custom.js"></script>
    @await RenderSectionAsync("js",required: false)
  </body>
</html>