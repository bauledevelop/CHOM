<!DOCTYPE html>
<html lang="en">

 
<head>

    <title>C.H.O.M</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0"  />
    <meta name="description" content="Chom Design" />
    <meta name="keywords" content="CHOMDesign">
    <meta name="author" content="chom">
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <link rel="icon" href="~/Client/dict/images/logo.svg" />
    <link rel="shortcut icon" href="~/Client/dict/images/logo.svg" type="image/x-icon" />
    <meta itemprop="name" content="CHOM Design" />
    <meta itemprop="image" content="https://chomdesign.com/Client/dict/images/logo.svg" />

    <meta property="og:type" content="website" />
    <meta property="og:title" content="CHOM Design" />
    <meta property="og:url" content="https://chomdesign.com/" />
    <meta property="og:site_name" content="CHOM Design" />
    <meta property="og:description" content="CHOM Design" />
    <meta property="og:image:alt" content="CHOM Design" />
    <meta property="og:image" content="http://photo-link-talk.zadn.vn/photolinkv2/720/aHR0cHM6Ly9jaG9tZGVzaWduLmNvbS9DbGllbnQvZGljdC9pbWFnZXMvbG9nby5zdmc=" />

    <meta name="twitter:card" content="https://chomdesign.com/" />
    <meta name="twitter:title" content="CHOM Design" />
    <meta name="twitter:image" content="https://chomdesign.com/Client/dict/images/logo.svg" />

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,300;0,400;0,500;0,700;0,900;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet">
    @await RenderSectionAsync("css",required: false)
</head>


<body class="hidden hidden-ball smooth-scroll" data-primary-color="#ff0000">
    <main>
        <!-- Preloader -->
        <div class="preloader-wrap" data-firstline="Thư" data-secondline="giãn">
            <div class="outer">
                <div class="inner">
                    <div class="trackbar">
                        <div class="loadbar"></div>
                    </div>

                    <div class="percentage-wrapper">
                        <div class="percentage" id="precent"></div>
                    </div>
                    <div class="percentage-intro">Đang tải</div>
                </div>
            </div>
        </div>
        <!--/Preloader -->

        <div class="cd-index cd-main-content">
            <!-- Page Content -->
            <div id="page-content" class="dark-content" data-bgcolor="#fafafa">

                <!-- Header -->
                @await Component.InvokeAsync("Header")
                <!--/Header -->
                <!-- Content Scroll -->
                @RenderBody()
                <!--/Content Scroll -->

                <div id="app"></div>
            </div>
            <!--/Page Content -->

        </div>
    </main>

    <div class="cd-cover-layer"></div>
    <div id="magic-cursor">
        <div id="ball">
            <div id="ball-drag-x"></div>
            <div id="ball-drag-y"></div>
            <div id="ball-loader"></div>
        </div>
    </div>
    <div id="clone-image">
        <div class="hero-translate"></div>
    </div>
    <div id="rotate-device"></div>
    <script src="~/Client/dict/js/jquery.min.js"></script>
    <script src="~/Client/dict/js/cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.5/ScrollMagic.min.js"></script>
    <script src="~/Client/dict/js/cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.5/plugins/animation.gsap.min.js"></script>
    <script src="~/Client/dict/js/cdnjs.cloudflare.com/ajax/libs/gsap/3.7.1/Draggable.min.js"></script>
    <script src="~/Client/dict/js/cdnjs.cloudflare.com/ajax/libs/gsap/3.7.1/ScrollTrigger.min.js"></script>
    <script src='~/Client/dict/js/cdnjs.cloudflare.com/ajax/libs/three.js/r83/three.js'></script>
    <script src='~/Client/dict/js/cdnjs.cloudflare.com/ajax/libs/jquery.imagesloaded/4.1.4/imagesloaded.pkgd.js'></script>
   
    <script src="~/Client/dict/js/clapatwebgl.js"></script>
    <script src="~/Client/dict/js/plugins.js"></script>
    <script src="~/Client/dict/js/common.js"></script>
    <script src="~/Client/dict/js/scripts.js"></script>
    @await RenderSectionAsync("js",required: false)
</body>

</html>